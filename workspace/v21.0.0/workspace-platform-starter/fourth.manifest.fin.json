{
	"devtools_port": 9090,
	"licenseKey": "openfin-demo-license-key",
	"runtime": {
		"arguments": "--enable-mesh --security-realm=fourth-workspace-starter-how-to-workspace-platform-starter",
		"version": "41.134.102.1"
	},
	"platform": {
		"uuid": "fourth-workspace-starter-how-to-workspace-platform-starter",
		"name": "Fourth Workspace Platform Starter",
		"icon": "https://built-on-openfin.github.io/workspace-starter/workspace/v21.0.0/workspace-platform-starter/common/favicon-32x32.png",
		"autoShow": false,
		"providerUrl": "https://built-on-openfin.github.io/workspace-starter/workspace/v21.0.0/workspace-platform-starter/shell/shell.html",
		"preventQuitOnLastWindowClosed": true,
		"appLogsTimezone": "utc",
		"enableAppLogging": false,
		"logManagement": {
			"enabled": false
		},
		"permissions": {
			"System": {
				"launchExternalProcess": true,
				"terminateExternalProcess": true,
				"downloadAsset": true,
				"openUrlWithBrowser": {
					"enabled": true,
					"protocols": ["mailto"]
				}
			}
		},
		"defaultWindowOptions": {
			"permissions": {
				"System": {
					"openUrlWithBrowser": {
						"enabled": true,
						"protocols": ["mailto"]
					}
				}
			},
			"viewVisibility": {
				"showViewsOnTabDrag": {
					"enabled": true
				},
				"showViewsOnSplitterDrag": {
					"enabled": true
				},
				"showViewsOnWindowResize": {
					"enabled": true
				}
			}
		},
		"defaultViewOptions": {},
		"interopBrokerConfiguration": {
			"logging": {
				"beforeAction": {
					"enabled": true
				},
				"afterAction": {
					"enabled": true
				}
			}
		}
	},
	"appAssets": [],
	"supportInformation": {
		"company": "OpenFin",
		"product": "Workspace Starter - Workspace Platform Starter - Fourth Client",
		"email": "support@openfin.co",
		"forwardErrorReports": true
	},
	"snapshot": {
		"windows": []
	},
	"customSettings": {
		"platformProvider": {
			"rootUrl": "https://built-on-openfin.github.io/workspace-starter/workspace/v21.0.0/workspace-platform-starter",
			"sharing": true,
			"initUrl": "https://built-on-openfin.github.io/workspace-starter/workspace/v21.0.0/workspace-platform-starter/js/provider.bundle.js"
		},
		"authProvider": {
			"modules": [
				{
					"title": "Example Authentication",
					"id": "example",
					"url": "https://built-on-openfin.github.io/workspace-starter/workspace/v21.0.0/workspace-platform-starter/js/modules/auth/example.bundle.js",
					"data": {
						"autoLogin": false,
						"loginUrl": "https://built-on-openfin.github.io/workspace-starter/workspace/v21.0.0/workspace-platform-starter/windows/modules/auth/example-login.html",
						"logoutUrl": "https://built-on-openfin.github.io/workspace-starter/workspace/v21.0.0/workspace-platform-starter/windows/modules/auth/example-logged-out.html",
						"authenticatedUrl": "https://built-on-openfin.github.io/workspace-starter/workspace/v21.0.0/workspace-platform-starter/windows/modules/auth/example-logged-in.html",
						"checkLoginStatusInSeconds": 1,
						"checkSessionValidityInSeconds": -1
					}
				}
			]
		},
		"endpointProvider": {
			"modules": [],
			"endpoints": [
				{
					"id": "platform-settings",
					"type": "fetch",
					"options": {
						"method": "GET",
						"url": "https://built-on-openfin.github.io/workspace-starter/workspace/v21.0.0/workspace-platform-starter/settings.json"
					}
				}
			]
		}
	}
}
