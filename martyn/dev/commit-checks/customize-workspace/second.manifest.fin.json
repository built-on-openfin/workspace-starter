				"url": "https://built-on-openfin.github.io/workspace-starter/martyn/dev/commit-checks/common/windows/intents/picker.html",
				"height": 400,
				"width": 400
			}
		},
		"appProvider": {
			"appsSourceUrl": "https://built-on-openfin.github.io/workspace-starter/martyn/dev/commit-checks/common/apps.json",
			"includeCredentialOnSourceRequest": "include",
			"cacheDurationInMinutes": 1,
			"appAssetTag": "appasset",
			"manifestTypes": ["view", "snapshot", "manifest", "external", "inline-view"]
		},
		"endpointProvider": {
			"endpoints": [
				{
					"id": "share-get",
					"type": "fetch",
					"options": {
						"method": "GET",
						"url": "https://workspace.openfin.co/api/share/[id]"
