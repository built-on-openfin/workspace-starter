{
	"$schema": "./schemas/manifest.schema.json",
	"devtools_port": 9090,
	"licenseKey": "openfin-demo-license-key",
	"runtime": {
		"arguments": "--v=1 --inspect --enable-mesh --security-realm=second-workspace-starter-how-to-customize-workspace",
		"version": "31.112.75.4"
	},
	"startup_app": {
		"name": "second-workspace-starter-how-to-customize-workspace"
	},
	"platform": {
		"uuid": "second-workspace-starter-how-to-customize-workspace",
		"name": "second-workspace-starter-how-to-customize-workspace",
		"icon": "https://built-on-openfin.github.io/workspace-starter/dev/martyn/notification-links/customize-workspace/favicon-32x32.png",
		"autoShow": false,
		"providerUrl": "https://built-on-openfin.github.io/workspace-starter/dev/martyn/notification-links/customize-workspace/platform/provider.html",
		"preventQuitOnLastWindowClosed": true,
		"permissions": {
			"System": {
				"launchExternalProcess": true,
				"terminateExternalProcess": true,
				"downloadAsset": true,
				"openUrlWithBrowser": {
					"enabled": true,
					"protocols": ["mailto"]
				}
			}
		},
		"defaultWindowOptions": {
			"permissions": {
				"System": {
					"openUrlWithBrowser": {
						"enabled": true,
						"protocols": ["mailto", "msteams", "tel"]
					}
				}
			}
		},
		"defaultViewOptions": {
			"permissions": {
				"System": {
					"openUrlWithBrowser": {
						"enabled": true,
						"protocols": ["mailto", "msteams", "tel"]
					}
				}
			}
		}
	},
	"snapshot": {
		"windows": []
	},
	"supportInformation": {
		"company": "OpenFin",
		"product": "Workspace Starter - Customize Workspace - Second Client",
		"email": "support@openfin.co",
		"forwardErrorReports": true
	},
	"customSettings": {
		"authProvider": {
			"modules": [
				{
					"id": "auth-example",
					"url": "https://built-on-openfin.github.io/workspace-starter/dev/martyn/notification-links/customize-workspace/js/modules/auth/example.bundle.js",
					"data": {
						"autoLogin": false,
						"loginUrl": "https://built-on-openfin.github.io/workspace-starter/dev/martyn/notification-links/customize-workspace/windows/modules/auth/example-login.html",
						"logoutUrl": "https://built-on-openfin.github.io/workspace-starter/dev/martyn/notification-links/customize-workspace/windows/modules/auth/example-logged-out.html",
						"authenticatedUrl": "https://built-on-openfin.github.io/workspace-starter/dev/martyn/notification-links/customize-workspace/windows/modules/auth/example-logged-in.html",
						"checkLoginStatusInSeconds": 1,
						"checkSessionValidityInSeconds": -1,
						"customData": {
							"users": [
								{ "name": "Josh Smith (Developer)", "email": "josh@smith.com", "role": "developer" },
								{ "name": "Sam Barns (Sales)", "email": "sam@barns.com", "role": "sales" }
							]
						}
					}
				}
			]
		},
		"endpointProvider": {
			"modules": [
				{
					"id": "auth-example-endpoint",
					"url": "https://built-on-openfin.github.io/workspace-starter/dev/martyn/notification-links/customize-workspace/js/modules/auth/example.bundle.js",
					"data": {
						"roleMapping": {
							"developer": {
								"excludeAppsWithTag": ["expero", "contact", "manager", "irs"],
								"preferredScheme": "dark"
							},
							"sales": {
								"excludeAppsWithTag": ["tools", "developer", "versions"],
								"preferredScheme": "light",
								"excludeMenuAction": ["developer-inspect", "raise-create-app-definition-intent"]
							}
						}
					}
				}
			],
			"endpoints": [
				{
					"id": "platform-settings",
					"type": "module",
					"typeId": "auth-example-endpoint",
					"options": {
						"method": "GET",
						"url": "https://built-on-openfin.github.io/workspace-starter/dev/martyn/notification-links/customize-workspace/settings.json"
					}
				}
			]
		}
	}
}
