{
	"devtools_port": 9090,
	"licenseKey": "openfin-demo-license-key",
	"runtime": {
		"arguments": "--v=1 --inspect",
		"version": "31.112.75.4"
	},
	"platform": {
		"uuid": "integrate-with-excel",
		"icon": "https://built-on-openfin.github.io/workspace-starter/dev/martyn/linting/integrate-with-excel/favicon.ico",
		"autoShow": false,
		"providerUrl": "https://built-on-openfin.github.io/workspace-starter/dev/martyn/linting/integrate-with-excel/platform/provider.html",
		"preventQuitOnLastWindowClosed": true,
		"permissions": {
			"System": {
				"downloadAsset": true,
				"launchExternalProcess": true,
				"openUrlWithBrowser": {
					"enabled": true,
					"protocols": ["mailto"]
				}
			}
		},
		"defaultWindowOptions": {
			"permissions": {
				"System": {
					"openUrlWithBrowser": {
						"enabled": true,
						"protocols": ["mailto"]
					}
				}
			}
		}
	},
	"shortcut": {
		"company": "OpenFin",
		"description": "A way of showing examples of what OpenFin can do.",
		"icon": "https://built-on-openfin.github.io/workspace-starter/dev/martyn/linting/integrate-with-excel/favicon.ico",
		"name": "Integrate With Excel - v13.0.0",
		"target": ["desktop", "start-menu"]
	},
	"appAssets": [
		{
			"alias": "excel-interop-example.xlsx",
			"version": "0.0.5",
			"src": "https://built-on-openfin.github.io/workspace-starter/dev/martyn/linting/integrate-with-excel/assets/excel-interop-example.zip",
			"target": "excel-interop-example.xlsx",
			"forceDownload": true
		}
	],
	"supportInformation": {
		"company": "OpenFin",
		"product": "Workspace Starter - Integrate With Excel - Client",
		"email": "support@openfin.co",
		"forwardErrorReports": true
	},
	"customSettings": {
		"excel": {
			"icon": "https://built-on-openfin.github.io/workspace-starter/dev/martyn/linting/integrate-with-excel/assets/excel.svg",
			"assets": [
				{
					"title": "Excel Interop Example",
					"description": "Demonstrate interop with Excel workbook",
					"workbook": "excel-interop-example.xlsx",
					"worksheets": [
						{
							"name": "Sheet1",
							"cellHandlers": [
								{
									"cell": "$B$3",
									"types": ["instrument", "fdc3.instrument"],
									"contextGroup": "green"
								},
								{
									"cell": "$B$4",
									"types": ["instrument", "fdc3.instrument"],
									"contextGroup": "purple"
								},
								{
									"cell": "$B$5",
									"types": ["instrument", "fdc3.instrument"],
									"contextGroup": "orange"
								},
								{
									"cell": "$B$6",
									"types": ["instrument", "fdc3.instrument"],
									"contextGroup": "red"
								},
								{
									"cell": "$B$7",
									"types": ["instrument", "fdc3.instrument"],
									"contextGroup": "pink"
								},
								{
									"cell": "$B$8",
									"types": ["instrument", "fdc3.instrument"],
									"contextGroup": "yellow"
								}
							]
						}
					]
				}
			]
		}
	}
}
