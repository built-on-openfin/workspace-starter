{
	"name": "openfin-workspace--integrate-with-rss",
	"version": "13.1.7",
	"description": "OpenFin Workspace -- Integrate with RSS",
	"main": "index.js",
	"scripts": {
		"dos": "node ./scripts/dos.mjs && node ./scripts/kill.mjs",
		"kill": "node ./scripts/kill.mjs",
		"client": "node ./scripts/launch.mjs",
		"build-client": "webpack build --config ./client/webpack.config.js --mode=development",
		"build-server": "tsc --project ./server",
		"build": "npm run build-server && npm run build-client",
		"start": "node ./server/build/index.js",
		"setup": "cd ../../ && npm install && cd how-to/integrate-with-rss && npm run build"
	},
	"author": "martyn.janes@openfin.co",
	"license": "SEE LICENSE IN LICENSE.MD",
	"dependencies": {
		"@openfin/workspace": "13.1.7",
		"@openfin/workspace-platform": "13.1.7",
		"fast-xml-parser": "4.2.7"
	},
	"devDependencies": {
		"@openfin/core": "31.75.4",
		"@types/express": "^4.17.17",
		"@types/node-fetch": "^2.6.4",
		"express": "^4.18.2",
		"openfin-adapter": "31.75.4",
		"node-fetch": "cjs",
		"ts-loader": "^9.4.4",
		"typescript": "^5.2.2",
		"webpack": "^5.88.2",
		"webpack-cli": "^5.1.4"
	}
}
