{
  "name": "automation-testing-register-with-home-js",
  "version": "19.1.0",
  "description": "OpenFin Workspace Starter Automation Tests",
  "scripts": {
    "build": "echo Nothing to build",
    "test-remote-mocha": "of-automation https://built-on-openfin.github.io/workspace-starter/workspace/v19.1.0/register-with-home/manifest.fin.json ./tests/**/*.spec.js --reporter ./mocha.reporter.json --testFailCount 1 --workspace 19.1.23",
    "test-local-mocha": "of-automation http://localhost:8080/manifest.fin.json ./tests/**/*.spec.js --workspace 19.1.23",
    "test-remote-jasmine": "of-automation https://built-on-openfin.github.io/workspace-starter/workspace/v19.1.0/register-with-home/manifest.fin.json ./tests/**/*.spec.js --framework jasmine --reporter ./jasmine.reporter.json --testFailCount 1 --workspace 19.1.23",
    "test-local-jasmine": "of-automation http://localhost:8080/manifest.fin.json ./tests/**/*.spec.js --framework jasmine --workspace 19.1.23",
    "test-remote-jest": "of-automation https://built-on-openfin.github.io/workspace-starter/workspace/v19.1.0/register-with-home/manifest.fin.json ./tests/**/*.spec.js --framework jest --reporter ./jest.reporter.json --testFailCount 1 --workspace 19.1.23",
    "test-local-jest": "of-automation http://localhost:8080/manifest.fin.json ./tests/**/*.spec.js --framework jest --workspace 19.1.23",
    "test-offline": "of-automation http://localhost:8080/manifest.fin.json ./tests/**/*.spec.js --offline --storageFolder=./offline-storage --workspace 19.1.23",
    "test-remote-mocha-selenium": "of-automation https://built-on-openfin.github.io/workspace-starter/workspace/v19.1.0/register-with-home/manifest.fin.json ./tests/**/*.spec.js --driver=selenium --workspace 19.1.23",
    "test-local-mocha-selenium": "of-automation http://localhost:8080/manifest.fin.json ./tests/**/*.spec.js --driver=selenium --workspace 19.1.23"
  },
  "author": "martyn.janes@openfin.co",
  "license": "SEE LICENSE IN LICENSE.MD",
  "devDependencies": {
    "@openfin/automation-cli": "^1.3.0",
    "@openfin/automation-helpers": "^1.3.0",
    "jasmine-pretty-html-reporter": "0.2.5",
    "jest-html-reporters": "3.1.7",
    "mochawesome": "7.1.3"
  }
}
