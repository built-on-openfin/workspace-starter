	"devDependencies": {
		"@openfin/automation-cli": "file:../packages/automation-cli",
		"@openfin/automation-helpers": "file:../packages/automation-helpers",
		"chai": "4.3.6",
		"jasmine-pretty-html-reporter": "0.2.5",
		"jest-html-reporters": "3.0.11",
		"mochawesome": "7.1.3"
	}
