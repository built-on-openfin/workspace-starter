{
	"name": "automation-testing-register-with-home-js",
	"version": "1.0.0",
	"description": "OpenFin Workspace Starter Automation Tests",
	"scripts": {
		"test-remote-mocha": "of-automation https://built-on-openfin.github.io/workspace-starter/workspace/v9.0.0/register-with-home/manifest.fin.json ./tests/9.0.0/**/*.spec.js --reporter ./mocha.reporter.json --testFailCount 2 --workspace 9.1.22",
		"test-remote-mocha-v8": "of-automation https://built-on-openfin.github.io/workspace-starter/workspace/v8.0.0/register-with-home/manifest.fin.json ./tests/8.0.0/**/*.spec.js --reporter ./mocha.reporter.json --testFailCount 2 --workspace 8.1.7",
		"test-local-mocha": "of-automation http://localhost:8080/manifest.fin.json ./tests/9.0.0/**/*.spec.js --workspace 9.1.22",
		"test-remote-jasmine": "of-automation https://built-on-openfin.github.io/workspace-starter/workspace/v9.0.0/register-with-home/manifest.fin.json ./tests/9.0.0/**/*.spec.js --framework jasmine --reporter ./jasmine.reporter.json --testFailCount 2 --workspace 9.1.22",
		"test-local-jasmine": "of-automation http://localhost:8080/manifest.fin.json ./tests/9.0.0/**/*.spec.js --framework jasmine --workspace 9.1.22",
		"test-remote-jest": "of-automation https://built-on-openfin.github.io/workspace-starter/workspace/v9.0.0/register-with-home/manifest.fin.json ./tests/9.0.0/**/*.spec.js --framework jest --reporter ./jest.reporter.json --testFailCount 2 --workspace 9.1.22",
		"test-local-jest": "of-automation http://localhost:8080/manifest.fin.json ./tests/9.0.0/**/*.spec.js --framework jest --workspace 9.1.22",
		"test-offline": "of-automation http://localhost:8080/manifest.fin.json ./tests/9.0.0/**/*.spec.js --offline --storageFolder=./offline-storage --workspace 9.1.22",
		"test-remote-mocha-selenium": "of-automation https://built-on-openfin.github.io/workspace-starter/workspace/v9.0.0/register-with-home/manifest.fin.json ./tests/9.0.0/**/*.spec.js --driver=selenium --workspace 9.1.22",
		"test-local-mocha-selenium": "of-automation http://localhost:8080/manifest.fin.json ./tests/9.0.0/**/*.spec.js --driver=selenium --workspace 9.1.22"
	},
