{
  "name": "openfin-workspace--customize-home-templates",
  "version": "6.0.0",
  "description": "OpenFin Workspace -- Register with Home",
  "main": "index.js",
  "scripts": {
    "dos": "desktop-owner-settings.bat && (npm run -s kill:fin || 1) && (npm run -s kill:rvm || 1)",
    "build": "npm run build-server & npm run build-client",
    "build-server": "tsc --project ./server",
    "build-client": "webpack build --config ./client/webpack.config.js --mode=development",
    "build-client-prod": "webpack build --config ./client/webpack.config.js --mode=production",
    "start": "npm run server",
    "client": "start fin://localhost:8080/manifest.fin.json",
    "server": "node ./server/build/index.js",
    "kill": "(npm run -s kill:fin || exit 0) && (npm run -s kill:rvm || exit 0)",
    "kill:fin": "cmd.exe /c taskkill /F /IM OpenFin.exe /T",
    "kill:rvm": "cmd.exe /c taskkill /F /IM OpenFinRVM.exe /T"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@openfin/workspace": "^6.3.6",
    "@openfin/workspace-platform": "^6.3.6",
    "chart.js": "^3.7.1",
    "csstype": "^3.0.11",
    "luxon": "^2.3.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/luxon": "^2.3.1",
    "@types/node": "^14.14.33",
    "express": "^4.17.1",
    "openfin-adapter": "^21.64.9",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.4",
    "webpack": "^5.51.1",
    "webpack-cli": "^4.8.0"
  }
}
