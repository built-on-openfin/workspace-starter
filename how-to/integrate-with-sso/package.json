{
  "name": "openfin-workspace--integrate-with-sso",
  "version": "8.0.0",
  "description": "OpenFin Workspace -- Integrate with SSO",
  "main": "index.js",
  "scripts": {
    "dos": "desktop-owner-settings.bat && (npm run kill)",
    "build": "npm run build-server & npm run build-client",
    "build-server": "tsc --project ./server",
    "build-client": "webpack build --config ./client/webpack.config.js --mode=development",
    "build-client-prod": "webpack build --config ./client/webpack.config.js --mode=production",
    "start": "npm run server",
    "client": "node ./scripts/launch.mjs",
    "server": "node ./server/build/index.js",
    "kill": "node ./scripts/kill.mjs"
  },
  "author": "",
  "license": "SEE LICENSE IN LICENSE.MD",
  "dependencies": {
    "@openfin/core": "^25.68.26",
    "@openfin/workspace": "next",
    "@openfin/workspace-platform": "next",
    "auth0-js": "^9.19.0"
  },
  "devDependencies": {
    "@types/auth0-js": "^9.14.6",
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.33",
    "express": "^4.17.1",
    "openfin-adapter": "^21.64.9",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.4",
    "webpack": "^5.51.1",
    "webpack-cli": "^4.8.0"
  }
}
