{
	"name": "openfin-workspace--integrate-with-auth0",
	"version": "12.6.0",
	"description": "OpenFin Workspace -- Integrate with Auth0",
	"main": "index.js",
	"scripts": {
		"dos": "npm --prefix ../../common/ run dos",
		"build": "npm run build-server & npm run build-client",
		"build-server": "tsc --project ./server",
		"build-client": "webpack build --config ./client/webpack.config.js --mode=development",
		"build-client-prod": "webpack build --config ./client/webpack.config.js --mode=production",
		"start": "npm run server",
		"client": "npm --prefix ../../common/ run client",
		"server": "node ./server/build/index.js",
		"kill": "npm --prefix ../../common/ run kill",
		"setup": "cd ../../../ && npm install && cd how-to/integrate-with-sso/integrate-with-auth0 && npm run build && npm --prefix ../../common/ run build",
		"validate": "npm --prefix ../../../ run validate"
	},
	"author": "martyn.janes@openfin.co",
	"license": "SEE LICENSE IN LICENSE.MD",
	"dependencies": {
		"@openfin/workspace": "12.6.7",
		"@openfin/workspace-platform": "12.6.7",
		"auth0-js": "^9.20.2"
	},
	"devDependencies": {
		"@openfin/core": "30.74.13",
		"@types/auth0-js": "^9.14.7",
		"@types/express": "^4.17.17",
		"@types/node": "^20.1.3",
		"express": "^4.18.2",
		"ts-loader": "^9.4.2",
		"typescript": "^4.9.5",
		"webpack": "^5.82.1",
		"webpack-cli": "^5.1.1"
	}
}
