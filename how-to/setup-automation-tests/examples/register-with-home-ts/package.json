{
    "name": "setup-automation-tests-register-with-home",
    "version": "1.0.0",
    "description": "OpenFin Workspace Starter Automation Tests",
    "scripts": {
        "test-local-mocha": "of-automation http://localhost:8080/manifest.fin.json ./tests/**/*.spec.ts",
        "test-remote-mocha": "of-automation https://built-on-openfin.github.io/workspace-starter/workspace/v6.0.0/register-with-home/manifest.fin.json ./tests/**/*.spec.ts",
        "test-local-jasmine": "of-automation http://localhost:8080/manifest.fin.json ./tests/**/*.spec.ts --framework jasmine",
        "test-remote-jasmine": "of-automation https://built-on-openfin.github.io/workspace-starter/workspace/v6.0.0/register-with-home/manifest.fin.json ./tests/**/*.spec.ts --framework jasmine",
        "test-local-jest": "of-automation http://localhost:8080/manifest.fin.json ./tests/**/*.spec.ts --framework jest",
        "test-remote-jest": "of-automation https://built-on-openfin.github.io/workspace-starter/workspace/v6.0.0/register-with-home/manifest.fin.json ./tests/**/*.spec.ts --framework jest",
        "test-offline": "of-automation http://localhost:8080/manifest.fin.json ./tests/**/*.spec.ts --offline --storageFolder=./offline-storage"
    },
