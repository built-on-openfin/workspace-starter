{
	"name": "integrate-with-bloomberg-basic",
	"version": "12.6.0",
	"description": "OpenFin Workspace - Example shows how to connect to a Bloomberg terminal and issue basic commands to drive it",
	"main": "index.js",
	"scripts": {
		"dos": "node ./scripts/dos.mjs && node ./scripts/kill.mjs",
		"kill": "node ./scripts/kill.mjs",
		"client": "node ./scripts/launch.mjs",
		"build-client": "webpack build --config ./client/webpack.config.js --mode=development",
		"build": "npm run build-client",
		"start": "npx --yes http-server ./public -p 8080 -c-1",
		"setup": "cd ../../ && npm install && cd how-to/integrate-with-bloomberg-basic && npm run build"
	},
	"keywords": [
		"bloomberg",
		"integration"
	],
	"author": "ameet.jayawant@openfin.co",
	"license": "SEE LICENSE IN LICENSE.MD",
	"dependencies": {
		"@openfin/bloomberg": "^1.1.0",
		"@openfin/core": "30.74.16",
		"@openfin/workspace": "12.6.7",
		"@openfin/workspace-platform": "12.6.7"
	},
	"devDependencies": {
		"@types/node": "^20.2.4",
		"openfin-adapter": "30.74.16",
		"ts-loader": "^9.4.3",
		"typescript": "^4.9.5",
		"webpack": "^5.84.1",
		"webpack-cli": "^5.1.1"
	}
}
