    "homeProvider": {
      "id": "support-context-and-intents",
      "title": "Home Starter",
      "icon": "http://localhost:8080/favicon.ico",
      "queryMinLength": 3,
      "queryAgainst":["title"]
    },
    "storefrontProvider": {
      "id": "support-context-and-intents",
      "title": "Custom Storefront",
      "icon": "http://localhost:8080/favicon.ico",
      "landingPage": {
        "hero": {
          "title": "Custom Hero Title",
