{
	"name": "openfin-workspace--support-context-and-intents",
	"version": "13.1.8",
	"description": "OpenFin Workspace Example Application -- Adding intent support to a workspace platform",
	"main": "index.js",
	"scripts": {
		"dos": "node ./scripts/dos.mjs && node ./scripts/kill.mjs",
		"kill": "node ./scripts/kill.mjs",
		"client": "node ./scripts/launch.mjs",
		"secondclient": "node ./scripts/launch.mjs http://localhost:8080/second.manifest.fin.json",
		"build-client": "webpack build --config ./client/webpack.config.js --mode=development",
		"build": "npm run build-client",
		"start": "npx --yes http-server ./public -p 8080 -c-1",
		"setup": "cd ../../ && npm install && cd how-to/support-context-and-intents && npm run build"
	},
	"author": "john.mandia@openfin.co",
	"license": "SEE LICENSE IN LICENSE.MD",
	"dependencies": {
		"@openfin/workspace": "13.1.8",
		"@openfin/workspace-platform": "13.1.8"
	},
	"devDependencies": {
		"@openfin/core": "31.75.4",
		"openfin-adapter": "31.75.4",
		"ts-loader": "^9.4.4",
		"typescript": "^5.2.2",
		"webpack": "^5.88.2",
		"webpack-cli": "^5.1.4"
	}
}
