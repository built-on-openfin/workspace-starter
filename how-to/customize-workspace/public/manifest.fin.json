				},
				{
					"include": true,
					"id": "show-hide-tabs",
					"button": {
						"type": "ShowHideTabs"
					}
				},
				{
					"include": true,
					"id": "color-linking",
					"button": {
						"type": "ColorLinking"
					}
				},
				{
					"include": true,
					"id": "preset-layouts",
					"button": {
						"type": "PresetLayouts"
					}
				},
				{
					"include": true,
					"id": "share",
					"button": {
						"type": "Custom",
						"tooltip": "Share",
						"disabled": false,
						"iconUrl": "http://localhost:8080/common/icons/{theme}/{scheme}/share.svg",
						"action": {
							"id": "share",
							"customData": {}
						}
					},
					"conditions": ["sharing"]
				},
				{
					"include": true,
					"id": "save-menu",
					"button": {
						"type": "SaveMenu"
					}
				}
			]
		},
