				"newPageUrl": "http://localhost:8080/common/views/platform/new-tab/new-tab.html"
			},
			"globalMenu": [
				{
					"include": true,
					"label": "Inspect Platform",
					"data": {
						"type": "Custom",
						"action": {
							"id": "developer-inspect",
							"customData": { "target": "platform" }
						}
					},
					"position": {
						"operation": "start"
					},
					"separator": "after"
				},
				{
					"include": true,
					"label": "Inspect Window",
					"data": {
						"type": "Custom",
						"action": {
							"id": "developer-inspect"
						}
					},
					"position": {
						"operation": "start"
					}
				},
				{
					"include": true,
					"label": "Toggle Notification Center",
					"data": {
						"type": "Custom",
						"action": {
							"id": "notification-toggle"
						}
					},
					"position": {
						"type": "OpenStorefront",
						"operation": "after"
					}
				},
				{
					"include": true,
					"label": "Open Home",
					"data": {
						"type": "Custom",
						"action": {
							"id": "home-show"
						}
					},
					"position": {
						"type": "OpenStorefront",
						"operation": "after"
					}
				},
				{
					"include": true,
					"label": "Quit App",
