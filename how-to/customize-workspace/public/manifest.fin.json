					"position": {
						"type": "Quit",
						"operation": "after"
					},
					"conditions": ["authenticated"]
				}
			],
			"pageMenu": [
				{
					"include": true,
					"label": "Move Page to new Window",
					"data": {
						"type": "Custom",
						"action": {
							"id": "move-page-to-new-window"
						}
					},
					"position": {
						"operation": "start"
					},
					"separator": "after"
				}
