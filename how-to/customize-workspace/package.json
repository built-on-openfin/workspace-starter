{
  "name": "openfin-workspace--customize-workspace",
  "version": "6.0.0",
  "description": "OpenFin Workspace Example Application -- A super set of the context and intents example this one shows you how to customize workspace.",
  "main": "index.js",
  "scripts": {
    "dos": "desktop-owner-settings.bat && (npm run -s kill:fin || true) && (npm run -s kill:rvm || true)",
    "build": "npm run build-server & npm run build-client",
    "build-server": "tsc --project ./server",
    "build-client": "webpack build --config ./client/webpack.config.js --mode=development",
    "build-client-prod": "webpack build --config ./client/webpack.config.js --mode=production",
    "start": "npm run server",
    "client": "start fin://localhost:8080/manifest.fin.json",
    "secondclient": "start fin://localhost:8080/second.manifest.fin.json",
    "server": "node ./server/build/index.js",
    "kill": "(npm run -s kill:fin || true) && (npm run -s kill:rvm || true)",
    "kill:fin": "cmd.exe /c taskkill /F /IM OpenFin.exe /T",
    "kill:rvm": "cmd.exe /c taskkill /F /IM OpenFinRVM.exe /T"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@openfin/native-window-integration-client": "0.0.13",
    "@openfin/workspace": "6.3.6",
    "@openfin/workspace-platform": "6.3.6",
    "file-loader": "^6.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.33",
    "express": "^4.17.1",
    "openfin-adapter": "^22.65.4",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.4",
    "webpack": "^5.51.1",
    "webpack-cli": "^4.8.0"
  }
}
