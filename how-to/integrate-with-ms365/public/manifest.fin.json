{
	"devtools_port": 9090,
	"licenseKey": "openfin-demo-license-key",
	"runtime": {
		"arguments": "--v=1 --inspect",
		"version": "28.106.72.14"
	},
	"platform": {
		"uuid": "integrate-with-ms365",
		"icon": "http://localhost:8080/favicon.ico",
		"autoShow": false,
		"providerUrl": "http://localhost:8080/platform/provider.html",
		"preventQuitOnLastWindowClosed": true,
		"permissions": {
			"System": {
				"openUrlWithBrowser": {
					"enabled": true,
					"protocols": ["msteams", "tel"]
				}
			}
		}
	},
	"snapshot": {
		"windows": []
	},
	"shortcut": {
		"company": "OpenFin",
		"description": "A way of showing examples of what OpenFin can do.",
		"icon": "http://localhost:8080/favicon.ico",
		"name": "Integrate With Microsoft 365 - v9.0",
		"target": ["desktop", "start-menu"]
	},
	"supportInformation": {
		"company": "OpenFin",
		"product": "Workspace Starter - Integrate With Microsoft 365 - Client",
		"email": "support@openfin.co",
		"forwardErrorReports": true
	},
	"customSettings": {
		"homeProvider": {
			"id": "integrate-with-ms365",
			"title": "Integrate with Microsoft 365",
			"icon": "http://localhost:8080/favicon.ico"
		},
		"browserProvider": {
			"windowOptions": {
				"title": "Microsoft 365",
				"icon": "http://localhost:8080/favicon.ico"
			}
		},
		"appProvider": {
			"appsSourceUrl": "http://localhost:8080/apps.json",
			"includeCredentialOnSourceRequest": "include",
			"cacheDurationInMinutes": 1,
			"appAssetTag": "appasset"
		},
		"integrationProvider": {
			"modules": [
				{
					"id": "ms365",
					"icon": "http://localhost:8080/favicon.ico",
					"title": "Microsoft 365",
					"enabled": true,
					"url": "http://localhost:8080/js/integrations/ms365.bundle.js",
					"data": {
						"clientId": "",
						"tenantId": "",
						"redirectUri": "http://localhost:8080/oauth_redirect.html",
						"permissions": [
							"User.Read",
							"Presence.Read",
							"Presence.Read.All",
							"Directory.Read.All",
							"Mail.ReadWrite",
							"Contacts.Read",
							"Tasks.Read",
							"Calendars.ReadWrite",
							"Chat.Read",
							"ChannelMessage.Read.All",
							"TeamMember.Read.All",
							"ChannelMember.Read.All"
						],
						"disableGraphExplorer": false,
						"enableLibLogging": false,
						"images": {
							"microsoft365": "http://localhost:8080/images/apps/microsoft365.svg",
							"teams": "http://localhost:8080/images/apps/teams.svg",
							"outlook": "http://localhost:8080/images/apps/outlook-mail.svg",
							"email": "http://localhost:8080/images/icons/email.svg",
							"calendar": "http://localhost:8080/images/icons/calendar.svg",
							"share": "http://localhost:8080/images/icons/share.svg",
							"chat": "http://localhost:8080/images/icons/chat.svg",
							"call": "http://localhost:8080/images/icons/call.svg",
							"contact": "http://localhost:8080/images/icons/contact.svg",
							"team": "http://localhost:8080/images/icons/team.svg",
							"channel": "http://localhost:8080/images/icons/channel.svg",
							"clock": "http://localhost:8080/images/icons/clock.svg",
							"check": "http://localhost:8080/images/icons/check.svg",
							"cross": "http://localhost:8080/images/icons/cross.svg"
						}
					}
				}
			]
		}
	}
}
