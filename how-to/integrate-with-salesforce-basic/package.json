{
	"name": "openfin-workspace--integrate-with-salesforce-basic",
	"version": "15.0.0",
	"description": "OpenFin Workspace -- Integrate with Salesforce - Basic",
	"main": "index.js",
	"scripts": {
		"dos": "node ./scripts/dos.mjs && node ./scripts/kill.mjs",
		"kill": "node ./scripts/kill.mjs",
		"client": "node ./scripts/launch.mjs",
		"build-client": "webpack build --config ./client/webpack.config.js --mode=development",
		"build": "npm run build-client",
		"start": "npx --yes http-server ./public -p 8080 -c-1",
		"setup": "npm install && npm run build"
	},
	"author": "martyn.janes@openfin.co",
	"license": "SEE LICENSE IN LICENSE.MD",
	"dependencies": {
		"@openfin/salesforce": "2.2.0",
		"@openfin/workspace": "15.0.0",
		"@openfin/workspace-platform": "15.0.0"
	},
	"devDependencies": {
		"@openfin/core": "32.76.10",
		"openfin-adapter": "32.76.10",
		"ts-loader": "^9.4.4",
		"typescript": "^5.2.2",
		"webpack": "^5.88.2",
		"webpack-cli": "^5.1.4"
	}
}
