{
	"cSpell.words": [
		"actioned",
		"appasset",
		"bootstrapper",
		"closedown",
		"companyname",
		"Contoso",
		"csstype",
		"favicons",
		"finos",
		"interopbroker",
		"libor",
		"maximizable",
		"minimizable",
		"Northwind",
		"openfin",
		"qlaunch",
		"rspack",
		"templating"
	],
	"cSpell.ignorePaths": ["**/.eslintrc.js", "**/settings.json", "**/package.json"],
	"files.eol": "\n",
	"json.schemas": [
		{
			"fileMatch": ["/apps.json", "/*.apps.fin.json", "/apps-*.json"],
			"url": "./how-to/workspace-platform-starter/public/schemas/platform-apps.schema.json"
		},
		{
			"fileMatch": ["/*.view.fin.json"],
			"url": "./how-to/workspace-platform-starter/public/schemas/view.schema.json"
		},
		{
			"fileMatch": ["/*.window.fin.json"],
			"url": "./how-to/workspace-platform-starter/public/schemas/window.schema.json"
		},
		{
			"fileMatch": ["/*.snapshot.fin.json"],
			"url": "./how-to/workspace-platform-starter/public/schemas/snapshot.schema.json"
		},
		{
			"fileMatch": ["/workspace-platform-starter/*/*.manifest.fin.json", "/workspace-platform-starter/*/manifest.fin.json"],
			"url": "./how-to/workspace-platform-starter/public/schemas/wps.manifest.schema.json"
		},
		{
			"fileMatch": ["manifest.fin.json", "*.manifest.fin.json", "!**/workspace-platform-starter/*/*manifest.fin.json"],
			"url": "./how-to/workspace-platform-starter/public/schemas/openfin.manifest.schema.json"
		}
	],
	"jest.rootPath": "./src",
	"jest.autoRun": "off"
}
